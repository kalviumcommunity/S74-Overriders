{
  "compilerOptions": {
    /* Language and Environment */
    "target": "ES2017",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    /* Strictness */
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    /* Module Resolution */
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    /* JSX */
    "jsx": "react-jsx",
    /* Base URL & Path Aliases */
    "baseUrl": ".",
"paths": {
  "@/*": ["./app/*"]
}
,
    /* Incremental */
    "incremental": true,
    "esModuleInterop": true,
    "plugins": [
      {
        "name": "next"
      }
    ]
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts",
    ".next/dev/types/**/*.ts"
  ],
  "exclude": [
    "node_modules"
  ]
}
